      </Item>
      <Item>
        <Filename Value="src\forms\ConfirmationDialogForm\ConfirmationDialogForm.pas"/>
        <UnitName Value="ConfirmationDialogForm"/>
      </Item>
      <Item>
        <Filename Value="src\forms\LoaderDialogForm\LoaderDialogForm.lfm"/>
        <Type Value="LFM"/>
      </Item>
      <Item>
        <Filename Value="src\forms\LoaderDialogForm\LoaderDialogForm.pas"/>
