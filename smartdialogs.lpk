      </SearchPaths>
    </CompilerOptions>
    <Files>
      <Item>
        <Filename Value="simpledialog.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="SimpleDialog"/>
      </Item>
      <Item>
        <Filename Value="confirmationdialog.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="ConfirmationDialog"/>
      </Item>
      <Item>
